#include <stdio.h>
#include <stdlib.h>
int main()
{
	int a[25],i,n,item;
	int found=0;
	printf("Enter n:");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
	printf("Enter element %d:",(i+1));
	scanf("%d",&a[i]);
	}
	printf("Enter item to search:");
	scanf("%d",&item);
	for(i=0;i<n;i++)
	{
	if(a[i]==item)
	{
	printf("Item found at position %d",i+1);
	found=1;
	break;
	}
	}
	if(!found)
	printf("Not found");
	
	return 0;
}
