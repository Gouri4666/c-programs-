#include <stdio.h>
#include <stdlib.h>

// Define structure for a node
struct Node {
    int data;
    struct Node *next;
};

int main()
 {
    struct Node *start = NULL, *ptr, *newNode;
    int n, i;

    printf("Enter number of nodes: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++) {
        // Allocate memory for new node
        newNode = (struct Node*)malloc(sizeof(struct Node));

        printf("Enter data for node %d: ", i+1);
        scanf("%d", &newNode->data);

 
        newNode->next = NULL;

        if(start == NULL) {
            // First node
            start = newNode;
            ptr = newNode;
        }
 else
 {
            // Link new node at the end
            ptr->next = newNode;
            ptr = newNode;
        }
    }

    // Display linked list
    printf("\nLinked List: ");
    ptr = start;
    while(ptr != NULL) 
{
        printf("%d ", ptr->data);
        ptr = ptr->next;
    }
    
	printf("NULL\n");

    return 0;
}
