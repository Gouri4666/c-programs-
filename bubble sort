#include<stdio.h>
#include<conio.h>
int main()
{
	int i,j,n,temp,a[50];	
	printf("Enter n:");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
	printf("Enter element %d:",i+1);
	scanf("%d",&a[i]);
	}
	for(i=0;i<n-1;i++)
	{
	for(j=0;j<n-i-1;j++)
	{
	if(a[j]>a[j+1])
		{
			temp=a[j];
			a[j]=a[j+1];
			a[j+1]=temp;
		}
	
	}
	printf("Iteration %d:",(i+1));
	for(int k=0;k<n;k++)
	printf("%d ",a[k]);
	printf("\n");
	}
	return 0;
}

