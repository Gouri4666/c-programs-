#include <stdio.h>
int main()
{
        int n,i,isprime=1;
    printf("Enter a no.:");
    scanf("%d",&n);
    if(n<=1)
    isprime=0;
    else
    {
      for(i=2;i<=n/2;i++)  
      {
      if(!(n%i))
      isprime=0;
      break;
      }
    }
    if(isprime)
    printf("prime");
    else
    printf("Not prime");
    return 0;
}
